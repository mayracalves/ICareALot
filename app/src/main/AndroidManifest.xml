<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.icarealot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ICareALot">
        <activity android:name=".DetalhesPhotosActivity"></activity>
        <activity android:name=".Photos" />
        <activity android:name=".DetalhesAlbumsActivity" />
        <activity android:name=".DetalhesCommentsActivity" />
        <activity android:name=".Albums" />
        <activity android:name=".Comments" />
        <activity android:name=".Post" />
        <activity android:name=".DetalhePostActivity" />
        <activity android:name=".Principal" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>